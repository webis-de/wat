					</descriptorRefs>
					<archive>
						<manifest>
							<mainClass>de.aitools.aq.wat.WatServletServer</mainClass>
						</manifest>
					</archive>
					<appendAssemblyId>true</appendAssemblyId>
					<finalName>WAT-SL-${project.version}</finalName>
					<outputDirectory>${project.basedir}</outputDirectory>
				</configuration>
				<executions>
					<execution>
						<id>make-assembly</id>
						<phase>package</phase>
						<goals>
							<goal>single</goal>
						</goals>
					</execution>
				</executions>
